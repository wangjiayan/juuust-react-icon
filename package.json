{
  "name": "test-juuust-react-icon",
  "version": "1.0.4",
  "description": "Icon automation workflow with Figma",
  "main": "dist/index.js",
  "typings": "dist/icons.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/wangjiayan/juuust-react-icon",
  "author": "leadream",
  "keywords": [
    "react",
    "icons",
    "svg",
    "inline",
    "figma",
    "juuust",
    "design"
  ],
  "bugs": {
    "url": "https://github.com/wangjiayan/juuust-react-icon/issues"
  },
  "homepage": "https://github.com/wangjiayan/juuust-react-icon#readme",
  "scripts": {
    "fetch": "node bin/fetchSVG.js",
    "generate": "sudo rm -rf src/icons && node bin/build.js",
    "build:bundle": "sudo rm -rf dist && node_modules/.bin/rollup --c ./build/rollup.config.bundle.js",
    "dev": "rollup -w -c ./build/rollup.config.dev.js",
    "build": "rollup -c ./build/rollup.config.prod.js",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --max-warnings 0",
      "git add"
    ]
  },
  "config": {
    "style": "fill",
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@material-ui/core": "^4.12.2",
    "juuust-react-icon": "^0.2.5",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "test-juuust-react-icon": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.3",
    "commitizen": "^4.2.4",
    "concurrently": "^4.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "figma-js": "^1.11.0",
    "fs-extra": "^9.0.1",
    "got": "^11.5.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "p-queue": "2.4.2",
    "prettier": "^2.4.1",
    "prettier-eslint": "^9.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-uglify": "^6.0.4",
    "svgo": "^1.3.0",
    "uppercamelcase": "^3.0.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "hooks": {
    "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
  }
}
